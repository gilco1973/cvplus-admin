{
  "name": "@cvplus/admin",
  "version": "1.0.0",
  "description": "Comprehensive admin dashboard module for CVPlus platform management",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@cvplus/core": "^1.0.0",
    "@cvplus/auth": "^1.0.0",
    "@cvplus/premium": "^1.0.0",
    "@cvplus/analytics": "^1.0.0",
    "@cvplus/public-profiles": "^1.0.0",
    "firebase-admin": "^12.0.0",
    "firebase": "^10.7.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cvplus/cvplus.git",
    "directory": "packages/admin"
  },
  "keywords": [
    "admin",
    "dashboard",
    "cvplus",
    "user-management",
    "content-moderation",
    "system-monitoring",
    "analytics",
    "security-audit"
  ],
  "author": "Gil Klainert <gil@cvplus.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  }
}